<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AreaChoiceActivity">

    <Button
        android:id="@+id/back_btn"
        style="@style/NoShadow"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:background="#FFFFFF"
        android:drawableLeft="@drawable/backbtn"
        android:text=""
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="409"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="지역 선택"
        android:textColor="#000000"
        android:textSize="28sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="10dp" />

    <TextView
        android:id="@+id/AreaView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="시/도"
        android:textColor="#000000"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="68dp" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="서울"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="138dp" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="경기도"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="204dp" />

    <Button
        android:id="@+id/btn3"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="인천"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="270dp" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="강원도"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="336dp" />

    <Button
        android:id="@+id/btn5"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="대전/충청"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="402dp" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="대구"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="468dp" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="부산/울산"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="534dp" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="경상"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="413dp"
        android:layout_height="67dp"
        android:background="@drawable/btn_line"
        android:text="광주/전라/제주"
        android:textColor="#000000"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="666dp" />

</androidx.constraintlayout.widget.ConstraintLayout>